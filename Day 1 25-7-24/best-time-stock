class Solution {
    public int maxProfit(int[] prices) {
        int lowest =100001;
        int maxp=0;
        int n = prices.length;
        for(int i=0;i<n;i++)
        {
            if(prices[i]<=lowest)
            {
                lowest=prices[i];
            }
            else
            {
                int p;
                p = prices[i] - lowest;
                maxp = Math.max(maxp,p);
            }
        }
        return maxp;
    }
}